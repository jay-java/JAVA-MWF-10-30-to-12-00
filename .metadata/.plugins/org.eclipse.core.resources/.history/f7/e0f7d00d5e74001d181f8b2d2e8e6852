import java.util.ArrayList;
import java.util.List;

public class ListDemo {
	public static void main(String[] args) {
//		<Entity->TYpe
//		<generics>
		List list = new ArrayList<>();
		list.add(1);
		list.add("name");
		list.add('a');
		list.add(565.5);
		list.add(false);
		list.add(12);
		list.add(-12);
		list.add(97567);
		System.out.println(list);
	}
}
