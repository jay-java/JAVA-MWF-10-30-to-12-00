class Mobile{
	int price;
	String com;
	public int getPrice() {
		return price;
	}
	public String getCom() {
		return com;
	}
	public Mobile(int price,String com) {
		this.price=price;
		this.com = com;
	}
	@Override
	public String toString() {
		return "price : "+price+" comapanyuy : "+com;
	}
}
